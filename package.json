      "types": "./types/flipbook.d.ts"
    }
  },
  "devDependencies": {
    "@vue/compiler-sfc": "*",
    "postcss": "*",
    "vite": "*",
    "@vitejs/plugin-vue": "*"
  },
  "peerDependencies": {
    "vue": ">=3.4.38"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 0.25%",
    "not dead"
  ],
  "files": [
    "dist",
    "types"
  ],
  "dependencies": {
    "rematrix": "*"
  }
}
